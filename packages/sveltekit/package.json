{
	"name": "@twinstar-bosskills/sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "PORT=3000 node --expose-gc build/index.js",
		"start:bun": "echo 'check RAM usage' && PORT=3000 bun run build/index.js",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check src",
		"format": "prettier --plugin-search-dir . --write src",
		"migrations:mysql:generate": "pnpm --filter drizzle-kit generate:mysql --config=drizzle.mysql.ts",
		"migrations:mysql:continue": "vite-node --options.transformMode.ssr='/.*/' ./src/lib/server/bin/migrate.mysql.ts",
		"cli:synchronize-with-api": "vite-node --options.transformMode.ssr='/.*/' ./src/lib/server/bin/synchronize-with-api.ts",
		"cli:synchronize-with-api:today": "vite-node --options.transformMode.ssr='/.*/' ./src/lib/server/bin/synchronize-with-api.ts --realms=Helios,Athena,Proudmoore,MoPPvE --from-date=`date +'%Y-%m-%d'`",
		"cli:clean-db": "vite-node --options.transformMode.ssr='/.*/' ./src/lib/server/bin/clean-db.ts",
		"cli:clean-cache": "vite-node --options.transformMode.ssr='/.*/' ./src/lib/server/bin/clean-cache.ts",
		"cli:cache-character-boss-rankings": "vite-node --options.transformMode.ssr='/.*/' ./src/lib/server/bin/cache-character-boss-rankings.ts",
		"cli:cache-character-boss-kill-percentiles": "vite-node --options.transformMode.ssr='/.*/' ./src/lib/server/bin/cache-character-boss-kill-percentiles.ts",
		"cli:cache-raid-lock-rankings": "vite-node --options.transformMode.ssr='/.*/' ./src/lib/server/bin/cache-raid-lock-rankings.ts",
		"cli:guild-token": "vite-node --options.transformMode.ssr='/.*/' ./src/lib/server/bin/guild-token.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^2.1.2",
		"@sveltejs/kit": "^2.46.4",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tanstack/svelte-table": "^8.21.3",
		"@twinstar-bosskills/api": "workspace:*",
		"@twinstar-bosskills/core": "workspace:*",
		"@twinstar-bosskills/db": "workspace:*",
		"@types/node": "^20.0.0",
		"commander": "^14.0.1",
		"dotenv": "^16.6.1",
		"drizzle-kit": "^0.19.13",
		"drizzle-orm": "^0.28.6",
		"echarts": "^5.6.0",
		"echarts-simple-transform": "^1.0.0",
		"ioredis": "^5.8.1",
		"json-stable-stringify": "^1.3.0",
		"mysql2": "^3.15.2",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.20",
		"svelte-adapter-bun": "^0.5.2",
		"svelte-check": "^3.8.6",
		"tslib": "^2.6.2",
		"typescript": "^5.9.3",
		"vite": "^5.0.0",
		"vite-node": "^0.34.6",
		"zod": "^4.1.12"
	},
	"type": "module",
	"dependencies": {
		"date-fns": "^2.30.0",
		"date-fns-tz": "^2.0.1"
	}
}
