{
  "name": "twinstar-bosskills",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel -r dev",
    "dev:libs": "pnpm --parallel --filter=!@twinstar-bosskills/sveltekit dev",
    "build": "pnpm -r build",
    "build:libs": "pnpm --parallel --filter=!@twinstar-bosskills/sveltekit build",
    "preview": "pnpm --filter @twinstar-bosskills/sveltekit preview",
    "start": "pnpm --filter @twinstar-bosskills/sveltekit start",
    "start:bun": "pnpm --filter @twinstar-bosskills/sveltekit start:bun",
    "check": "pnpm --parallel -r check",
    "check:watch": "pnpm --filter @twinstar-bosskills/sveltekit check:watch",
    "lint": "pnpm --filter @twinstar-bosskills/sveltekit lint",
    "format": "pnpm --parallel -r format",
    "migrator": "pnpm --filter @twinstar-bosskills/db migrator",
    "migrations:mysql:continue": "pnpm --filter @twinstar-bosskills/db migrator up",
    "cli:synchronize-with-api": "pnpm --filter @twinstar-bosskills/sveltekit cli:synchronize-with-api",
    "cli:synchronize-with-api:today": "pnpm --filter @twinstar-bosskills/sveltekit cli:synchronize-with-api:today",
    "cli:clean-db": "pnpm --filter @twinstar-bosskills/cli clean-db",
    "cli:clean-cache": "pnpm --filter @twinstar-bosskills/sveltekit cli:clean-cache",
    "cli:cache-character-boss-rankings": "pnpm --filter @twinstar-bosskills/sveltekit cli:cache-character-boss-rankings",
    "cli:cache-character-boss-kill-percentiles": "pnpm --filter @twinstar-bosskills/sveltekit cli:cache-character-boss-kill-percentiles",
    "cli:cache-raid-lock-rankings": "pnpm --filter @twinstar-bosskills/sveltekit cli:cache-raid-lock-rankings",
    "cli:guild-token": "pnpm --filter @twinstar-bosskills/sveltekit cli:guild-token"
  },
  "type": "module",
  "devDependencies": {
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
