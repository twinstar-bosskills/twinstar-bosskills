{
	"name": "sveltekills",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"migrations:generate": "drizzle-kit generate:sqlite",
		"migrations:continue": "echo 'does not work with node:20' && node --loader esbuild-register/loader -r esbuild-register --no-warnings ./src/lib/server/bin/migrate.ts",
		"synchronize-with-api": "node --loader esbuild-register/loader -r esbuild-register --no-warnings ./src/lib/server/bin/synchronize-with-api.ts"
	},
	"devDependencies": {
		"@libsql/client": "^0.3.5",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.22.6",
		"@tanstack/svelte-table": "^8.9.6",
		"@types/d3-array": "^3.0.5",
		"@types/d3-format": "^3.0.1",
		"@types/d3-quadtree": "^3.0.2",
		"@types/d3-scale": "^4.0.3",
		"@types/d3-time-format": "^4.0.0",
		"@vercel/analytics": "^1.0.2",
		"date-fns": "^2.30.0",
		"date-fns-tz": "^2.0.0",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.19.13",
		"drizzle-orm": "^0.28.6",
		"echarts": "^5.4.3",
		"echarts-simple-transform": "^1.0.0",
		"esbuild": "^0.19.4",
		"esbuild-register": "^3.5.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"tslib": "^2.6.2",
		"typescript": "^5.1.6",
		"vite": "^4.4.9"
	},
	"type": "module"
}
